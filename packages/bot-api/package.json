{
  "name": "@inline-chat/bot-api",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun run --cwd ../bot-api-types build && tsc -p tsconfig.json",
    "typecheck": "bun run --cwd ../bot-api-types build && tsc -p tsconfig.json --noEmit",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@inline-chat/bot-api-types": "^0.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@vitest/coverage-v8": "4.0.16",
    "typescript": "^5.8.3",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  }
}
